{
  "name": "site",
  "version": "0.0.1",
  "description": "Site to test Gatsby with WordPress as a CMS",
  "main": "gatsby-config.js",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:debug": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage=true",
    "test:update": "jest --verbose --updateSnapshot"
  },
  "author": "Scott W",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.33",
    "gatsby-wordpress-theme-phoenix": "*",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {}
}
